                <div class="home-form-col">
                    <div id="home-shade" style="display: none;"></div>
                    <div class="home-form" data-coverage="Europe, Americas, Asia, South Pacific, & Africa">
                        <div class="clearfix home-form-group">
                            @using (Html.BeginForm("DayByDay", "Plan", FormMethod.Post, new { id = "newPlanForm", @class = "planning-form expanded", autocomplete = "off" }))
                            {
                                <div class="steps">
                                    <div class="title " title="">
                                        Itinerary Planner
                                    </div>
                                    <div class="dests">
                                        <div class="destinations ui-front">
                                            <div class="destination">
                                                @Html.DropDownList("ProvinceId", EnumHelper.GetSelectList(typeof(DayScheduling.Entities.Plan.Provinces)), "Select Province", new { @class = "mydropdownlist" })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="dates clearfix">
                                        @*@Html.TextBox("PlanBeginTime", "",new { @class="flat", placeholder = "Begin Date Time" })
                                            @Html.TextBox("PlanEndTime", "", new { @class = "flat", placeholder = "End Date Time" })*@
                                        @Html.TextBox("BudgetInfo", "", new { @class = "flat", placeholder = "Budget" })
                                        <br /><br />
                                        @Html.TextBox("NumberOfFriends", "", new { @class = "flat", placeholder = "Number Of Friends" })
                                    </div>
                                    <div class="section-title more-options-btn expandable">Activities<span class="optional"> (optional)</span></div>
                                    <div class="step-content">
                                        <div class="style clearfix">
                                            <div class="column3"><input type="radio" id="newplan-stylePopular" class="stylePopular flat" name="style" value="1" checked /><label title="Popular" for="newplan-stylePopular">Popular</label></div>
                                            <div class="column3"><input type="radio" id="newplan-styleBalanced" class="styleBlanaced flat" name="style" value="2" /><label title="Balanced" for="newplan-styleBalanced">Balanced</label></div>
                                            <div class="column3"><input type="radio" id="newplan-styleHiddenGems" class="styleHiddenGems flat" name="style" value="3" /><label title="Hidden gems" for="newplan-styleHiddenGems">Hidden gems</label></div>
                                        </div>
                                        <div class="interests">
                                            <div class="column3"><input type="checkbox" id="newplan-interestCulture" class="interest interestCulture flat" name="categoryGroupNames" value="culture" /><label title="Culture" for="newplan-interestCulture">Culture</label></div>
                                            <div class="column3"><input type="checkbox" id="newplan-interestOutdoor" class="interest interestOutdoor flat" name="categoryGroupNames" value="outdoors" /><label title="Outdoors" for="newplan-interestOutdoor">Outdoors</label></div>
                                            <div class="column3"><input type="checkbox" id="newplan-interestUnwind" class="interest interestUnwind flat" name="categoryGroupNames" value="unwind" /><label title="Relaxing" for="newplan-interestUnwind">Relaxing</label></div>
                                            <div class="column3"><input type="checkbox" id="newplan-interestFun" class="interest interestRomantic flat" name="categoryGroupNames" value="fun" /><label title="Fun" for="newplan-interestFun">Fun</label></div>
                                            <div class="column3"><input type="checkbox" id="newplan-category10" class="flat" name="categoryGroupNames" value="Beaches" /><label title="Beaches" for="newplan-category10">Beaches</label></div>
                                            <div class="column3"><input type="checkbox" id="newplan-category26" class="flat" name="categoryGroupNames" value="Historic Sites" /><label title="Historic sites" for="newplan-category26">Historic sites</label></div>
                                            <div class="column3"><input type="checkbox" id="newplan-category4" class="flat" name="categoryGroupNames" value="Shopping" /><label title="Shopping" for="newplan-category4">Shopping</label></div>
                                            <div class="column3"><input type="checkbox" id="newplan-category19" class="flat" name="categoryGroupNames" value="Generic" /><label title="Generic" for="newplan-category19">Generic</label></div>
                                        </div>
                                    </div>
                                    <div class="section-title more-options-btn expandable">Do you want food or drink in your plan?<span class="optional"> (optional)</span><i id="arrow" class="arrow fa fa-angle-down"></i></div>
                                    <div id="fooddiv" class="step-content">
                                        <div class="interests">
                                            <div class="column3">
                                                @Html.DropDownList("FoodCategory", EnumHelper.GetSelectList(typeof(DayScheduling.Entities.Plan.FoodCategories)), "Food Place", new { @class = "mydropdownlist1" })
                                            </div>
                                            <div class="column3">&nbsp;</div>
                                            <div class="column3">
                                                @Html.DropDownList("AlcoholCategory", EnumHelper.GetSelectList(typeof(DayScheduling.Entities.Plan.AlcoholCategories)), "Alcohol Place", new { @class = "mydropdownlist1" })
                                            </div>
                                        </div>
                                    </div>
                                    <button type="submit" class="plan-button cta-button large" value="Submit">See your plan</button>
                                </div>
                                    <input type="hidden" name="requiredAttractionId" value="" />
                                    <div>
                                        <input type="hidden" name="_csrf" value="0cc231fc-9c37-453e-8c03-6422a07a69a7" />
                                    </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
