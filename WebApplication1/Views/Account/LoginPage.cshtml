                url: '@Url.Action("isLoginSuccess","Home")',
                data : "{NameOrEmail:email, password:pass}",
                dataType: 'json',
                crossDomain: true,
                success: function (data) {
                    window.location.href = data;
                }
            })*@
            $.post('@Url.Action("isLoginSuccess", "Account")', { NameOrEmail: email, password: pass }, function (data) {
                if (data.Success == "True") {
                    window.location.replace(data.TargetUrl)
                }
                else {
                    $('#errorContainer').show();
                    $('#loginPasswordError').show();
                    $('#loginPasswordError').text(data.Message);
                }

